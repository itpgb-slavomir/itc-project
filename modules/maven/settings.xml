<?xml version="1.0" encoding="UTF-8"?>
<settings>

	<pluginGroups>
		<pluginGroup>org.sonatype.plugins</pluginGroup>
		<pluginGroup>org.jboss.maven.plugins</pluginGroup>
	</pluginGroups>

	<servers>

		<server>
			<id>sk.itcloud.releases</id>
			<username>admin</username>
			<password>admin123</password>
		</server>

		<server>
			<id>sk.itcloud.snapshots</id>
			<username>admin</username>
			<password>admin123</password>
		</server>

		<server>
			<id>sk.synapse.releases</id>
			<username>admin</username>
			<password>admin123</password>
		</server>

		<server>
			<id>sk.synapse.snapshots</id>
			<username>admin</username>
			<password>admin123</password>
		</server>

		<server>
			<id>org.bishub.releases</id>
			<username>admin</username>
			<password>admin123</password>
		</server>

		<server>
			<id>org.bishub.snapshots</id>
			<username>admin</username>
			<password>admin123</password>
		</server>
	</servers>

	<profiles>

		<profile>
			<id>sk.itcloud</id>

			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>

			<repositories>

				<repository>
					<id>sk.itcloud.snapshots</id>
					<name>ITCloud Snapshots</name>

					<releases>
						<enabled>false</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>

					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>fail</checksumPolicy>
					</snapshots>

					<url>http://nexus.${project.host}/content/repositories/sk.itcloud.snapshots</url>
					<layout>default</layout>

				</repository>

				<repository>
					<id>sk.itcloud.releases</id>
					<name>ICT Releases</name>

					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>

					<snapshots>
						<enabled>false</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>fail</checksumPolicy>
					</snapshots>

					<url>http://nexus.${project.host}/content/repositories/sk.itcloud.releases</url>
					<layout>default</layout>

				</repository>

				<repository>
					<id>repository.jboss.org</id>
					<name>JBoss Repository</name>
					<layout>default</layout>
					<url>http://nexus.${project.host}/content/repositories/repository.jboss.org/</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>

			</repositories>

			<properties>
				<sonar.jdbc.url>jdbc:mysql://sonar.itc/sonar</sonar.jdbc.url>
				<sonar.jdbc.driver>com.mysql.jdbc.Driver</sonar.jdbc.driver>
				<sonar.jdbc.username>sonar</sonar.jdbc.username>
				<sonar.jdbc.password>sonar</sonar.jdbc.password>
				<sonar.host.url>http://sonar.itc</sonar.host.url>
			</properties>

		</profile>

		<profile>
			<id>org.bishub</id>

			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>

			<repositories>

				<repository>
					<id>org.bishub.snapshots</id>
					<name>Bishub Snapshots</name>

					<releases>
						<enabled>false</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>

					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>fail</checksumPolicy>
					</snapshots>

					<url>http://nexus.${project.host}/content/repositories/org.bishub.snapshots</url>
					<layout>default</layout>

				</repository>

				<repository>
					<id>org.bishub.releases</id>
					<name>Bishub Releases</name>

					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>

					<snapshots>
						<enabled>false</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>fail</checksumPolicy>
					</snapshots>

					<url>http://nexus.${project.host}/content/repositories/org.bishub.releases</url>
					<layout>default</layout>

				</repository>

			</repositories>

			<properties>
				<sonar.jdbc.url>jdbc:mysql://sonar.itc/sonar</sonar.jdbc.url>
				<sonar.jdbc.driver>com.mysql.jdbc.Driver</sonar.jdbc.driver>
				<sonar.jdbc.username>sonar</sonar.jdbc.username>
				<sonar.jdbc.password>sonar</sonar.jdbc.password>
				<sonar.host.url>http://sonar.itc</sonar.host.url>
			</properties>

		</profile>

		<profile>
			<id>profile-php-maven</id>

			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>

			<pluginRepositories>
				<pluginRepository>
					<id>release-repo1.php-maven.org</id>
					<name>PHP-Maven 2 Release Repository</name>
					<url>http://repos.php-maven.org/releases</url>
					<releases>
						<enabled>true</enabled>
					</releases>
				</pluginRepository>
				<pluginRepository>
					<id>snapshot-repo1.php-maven.org</id>
					<name>PHP-Maven 2 Snapshot Repository</name>
					<url>http://repos.php-maven.org/snapshots</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>

			<repositories>
				<repository>
					<id>release-repo1.php-maven.org</id>
					<name>PHP-Maven 2 Release Repository</name>
					<url>http://repos.php-maven.org/releases</url>
					<releases>
						<enabled>true</enabled>
					</releases>
				</repository>
				<repository>
					<id>snapshot-repo1.php-maven.org</id>
					<name>PHP-Maven 2 Snapshot Repository</name>
					<url>http://repos.php-maven.org/snapshots</url>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
		</profile>
	</profiles>

	<!-- <activeProfiles> <activeProfile></activeProfile> </activeProfiles> -->
</settings>